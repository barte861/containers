<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="400.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/2.2" fx:controller="com.olczak.gui.PopupView">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="194.0" minWidth="10.0" prefWidth="104.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="301.0" minWidth="10.0" prefWidth="296.0" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label text="Label1" />
      <Label text="Label2" GridPane.rowIndex="1" />
      <Label text="Label3" GridPane.rowIndex="2" />
      <Label text="Label4" GridPane.rowIndex="3" />
      <ChoiceBox fx:id="categoryChoice" prefWidth="150.0" GridPane.columnIndex="1" />
      <TextField fx:id="partName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <TextField fx:id="partCost" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <TextArea fx:id="partDetails" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Button onAction="#handleOk" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="ok" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <Button onAction="#handleCancel" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="cancel" GridPane.columnIndex="1" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets left="100.0" />
         </GridPane.margin>
      </Button>
   </children>

</GridPane>
